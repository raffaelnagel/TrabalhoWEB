	<%= form_for(@timestamp) do |f| %>
  <% if @timestamp.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@timestamp.errors.count, "error") %> prohibited this timestamp from being saved:</h2>

      <ul>
      <% @timestamp.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  
  
<table> 
  <tr>
	
		<div class="form-inputs">
			<th>
				<div class="form-inputs">
					<%= f.label :members_id %><br>
					<%= f.select :members_id, Member.all.collect{|u| [u.name, u.id]}  %>
				</div>
			</th>
			<th>
				<div class="form-inputs">
					<%= f.label :projects_id %><br>
					<%= f.select :projects_id, Project.all.collect{|u| [u.name, u.id]}  %>
				</div>
			</th>
			<th>
				<div class="form-inputs">
					<%= f.label :hours %><br>
					<%= f.text_field :hours %>
				</div>
			</th>
			<th>
				<div class="form-inputs">
					<%= f.label :date %><br>
					<%= f.datetime_select :date %>
				</div>
			</th>
		</div>
		<th>
			<div class="form-actions">
				<%= f.submit %>
			</div>
		</th>
  </tr>
</table>
<% end %>
