<h1>Listing Members</h1>
 
<table>
 <tr>
    <th>Name</th>
    <th>Leader</th>
    <th>Role</th>
    
  </tr>
<% @members.each do |member| %>
 
    <td><%= member.name %></td> 
    <td><%= member.isleader %></td> 
    <td><%= member.role_id %></td>      
   
      
  </tr>
<% end %>
</table>

<br><%= link_to 'Back', members_path %>